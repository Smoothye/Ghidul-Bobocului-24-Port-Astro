---
const { pageNumber, img } = Astro.props;
---

<div id=`page${pageNumber}-narrow` class="page">
    <div class="content-narrow">
        <!-- pagina {pageNumber} -->
        <img src={img} width="100%" height="100%">
    </div>
    <div class="right-arrow"> &rarr; </div>
    <div class="left-arrow"> &larr; </div>
</div>
